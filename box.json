{
    "directories": [
        "src"
    ],
    "files": [
        "VERSION",
        "vendor/typhonius/acquia-php-sdk-v2/VERSION"
    ],
    "finder": [
        {
            "name": "{\\.(php|bash|fish|zsh)}",
            "exclude": ["Tests"],
            "in": ["vendor", "bin"]
        }
    ],
    "git-version": "package_version",
    "main": "bin/logstream",
    "output": "logstream.phar"
}
